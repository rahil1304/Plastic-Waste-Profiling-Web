  "description": "PWP Object Detection",
  "main": "index.js",
  "engines": {
    "node": "8.x.x",
    "npm": "6.4.1",
    "yarn": "1.x"
  },
  "dependencies": {
    "@tensorflow/tfjs": "0.13.3",
    "animate.css": "^3.7.0",
    "firebase": "^5.8.1",
    "jquery": "^3.5.0",
